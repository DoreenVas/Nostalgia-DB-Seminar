<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ResultsController">
   <children>
      <TableView fx:id="results" layoutY="64.0" prefHeight="303.0" prefWidth="574.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="40.0" />
      <HBox alignment="CENTER" layoutY="350.0" AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="goBackButton" mnemonicParsing="false" onAction="#goBack" text="&lt;-" />
            <Button mnemonicParsing="false" onAction="#display" text="Display selected">
               <HBox.margin>
                  <Insets left="20.0" right="20.0" />
               </HBox.margin></Button>
            <Button fx:id="goForwardButton" mnemonicParsing="false" onAction="#goForward" text="-&gt;" />
         </children>
      </HBox>
      <ImageView fitHeight="30.0" fitWidth="40.0" layoutX="18.0" layoutY="11.0" onMouseClicked="#home" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="5.0">
         <image>
            <Image url="GUI\pic\home.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="30.0" fitWidth="40.0" layoutX="49.0" layoutY="5.0" onMouseClicked="#simpleSearch" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="5.0">
         <image>
            <Image url="GUI\pic\back.png" />
         </image></ImageView>
      <ImageView fitHeight="30.0" fitWidth="50.0" layoutX="542.0" layoutY="5.0" onMouseClicked="#backToTheFuture" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="17.0" AnchorPane.topAnchor="5.0">
         <image>
            <Image url="GUI\pic\backToTheFuture.png" />
         </image></ImageView>
      <Label fx:id="countLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="103.0" layoutY="12.0" text="Displaying 0 results" AnchorPane.topAnchor="12.0" />
   </children>
</AnchorPane>
