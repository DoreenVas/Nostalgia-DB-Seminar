<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="holder" prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.WaitingController">
   <children>
      <ImageView fx:id="loadingGif" fitHeight="126.0" fitWidth="127.0" layoutX="112.0" layoutY="95.0" onMouseClicked="#click" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pic/animatedEllipse.gif" />
         </image>
      </ImageView>
      <Label fx:id="loading" layoutX="141.0" layoutY="257.0" text="Loading...">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="tempB" mnemonicParsing="false" onAction="#click" text="Button" visible="false" />
   </children>
</AnchorPane>
